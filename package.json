{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=6.0.0"
  },
  "scripts": {
    "dev": "nuxt dev",
    "test": "vitest",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lintf": "eslint . --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "release": "standard-version",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V",
    "clean:cache": " rimraf .eslintcache && rimraf node_modules && pnpm install"
  },
  "dependencies": {
    "@nuxt-alt/proxy": "^2.4.7",
    "@nuxtjs/robots": "^3.0.0",
    "@unocss/reset": "^0.57.7",
    "axios": "^1.6.2",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.1.0",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@nuxt/devtools": "^1.0.3",
    "@nuxtjs/pwa": "^3.3.5",
    "@types/node": "^20.10.0",
    "@unocss/eslint-config": "^0.57.7",
    "@unocss/eslint-plugin": "^0.57.7",
    "@unocss/nuxt": "^0.57.7",
    "@unocss/preset-rem-to-px": "^0.57.7",
    "@vueuse/core": "^10.6.1",
    "@vueuse/nuxt": "^10.6.1",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "lint-staged": "^15.1.0",
    "nuxt": "^3.8.2",
    "nuxt-simple-sitemap": "^4.1.6",
    "postcss": "^8.4.31",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "standard-version": "^9.5.0",
    "unplugin-auto-import": "^0.17.1",
    "unplugin-vue-components": "^0.25.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lintf",
    "commit-msg": "pnpm run commitlint"
  }
}
