{
  "name": "nuxt-app",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=6.0.0"
  },
  "scripts": {
    "start": "nuxt dev",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "release": "standard-version",
    "commitlint": "commitlint --config commitlint.config.js -e -V",
    "clean:cache": "rimraf node_modules && rimraf .eslintcache && pnpm install"
  },
  "dependencies": {
    "@nuxt-alt/proxy": "^2.3.2",
    "@nuxtjs/robots": "^3.0.0",
    "@unocss/reset": "^0.51.13",
    "axios": "^1.6.0",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@nuxt/devtools": "^0.4.6",
    "@nuxtjs/pwa": "^3.3.5",
    "@types/node": "^18.18.7",
    "@unocss/eslint-config": "^0.56.5",
    "@unocss/nuxt": "^0.51.13",
    "@unocss/preset-rem-to-px": "^0.51.13",
    "@vueuse/core": "^10.5.0",
    "@vueuse/nuxt": "^10.5.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "element-plus": "^2.4.1",
    "lint-staged": "^15.0.2",
    "nuxt": "^3.8.0",
    "nuxt-simple-sitemap": "^2.7.0",
    "postcss": "^8.4.31",
    "simple-git-hooks": "^2.9.0",
    "standard-version": "^9.5.0",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint:fix",
    "commit-msg": "pnpm run commitlint"
  }
}
